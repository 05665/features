{
    "id": "go",
    "name": "Go",
    "options": {
        "version": {
            "type": "string",
            "proposals": ["latest", "none", "1.18", "1.17"],
            "default": "latest",
            "description": "Select or enter a Go version to install"
        }
    },
    "init": true,
    "extensions": ["golang.Go"],
    "containerEnv": {
        "GOPATH": "/usr/local/go",
        "PATH": "${GOPATH}/bin:${PATH}"
    },
    "capAdd": [ "SYS_PTRACE" ],
    "securityOpt": [ "seccomp=unconfined" ],
    "install": {
        "app": "",
        "file": "install.sh"
    }
}
